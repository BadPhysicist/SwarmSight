<Window x:Class="SwarmSight.ParameterSetter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SwarmSight"
        mc:Ignorable="d"
        Title="ParameterSetter" Height="300" Width="300">
    <Grid>
        <Image x:Name="canvas" Margin="0" Stretch="Fill"/>
        <Slider x:Name="contrastThreshold" Margin="0" VerticalAlignment="Bottom" Maximum="255" Value="70" ValueChanged="contrastThreshold_ValueChanged"/>
    </Grid>
</Window>
